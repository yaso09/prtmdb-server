{
    "version": 2,
    "installCommand": "pip install -r requirements.txt && npm i -g @google/clasp",
    "buildCommand": "",
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/main"
        }
    ]
}